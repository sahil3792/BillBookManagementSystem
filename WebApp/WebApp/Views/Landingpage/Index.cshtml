<script>
    let timerInterval;

    function requestOtp() {
        const emailInput = document.getElementById('email').value;

        $.ajax({
            url: '/Landingpage/RequestOtp?email=' + encodeURIComponent(emailInput),
            type: 'POST',
            contentType: 'application/json',
            success: function (data) {
                if (data.success) {
                    alert(data.message);
                    startTimer();
                    document.getElementById('otpContainer').style.display = 'block';
                    document.getElementById('sendotp').style.display = 'none';
                    document.getElementById('check').style.display = 'none';

                } else {
                    alert('Error: ' + data.message);
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {
                console.error('Error:', textStatus, errorThrown);
                alert('An error occurred while sending the OTP.');
            }
        });
    }


    function startTimer() {
        let timeLeft = 120;
        updateTimerDisplay(timeLeft);

        timerInterval = setInterval(function () {
            timeLeft--;
            updateTimerDisplay(timeLeft);

            if (timeLeft <= 0) {
                clearInterval(timerInterval);
                alert('Time is up! Please request a new OTP.');
                resetForm();
            }
        }, 1000);
    }

    function updateTimerDisplay(timeLeft) {
        const timerDisplay = document.getElementById('timer');
        const minutes = Math.floor(timeLeft / 60);
        const seconds = timeLeft % 60;
        timerDisplay.textContent = `You have ${minutes}m ${seconds}s to enter the OTP.`;
        timerDisplay.style.display = 'block';
    }

    function verifyOtp() {
        const otpInput = document.getElementById('otp').value;
       

        $.ajax({
            url: '/Landingpage/VerifyOtp?otp=' + encodeURIComponent(otpInput), // Pass OTP in the URL
            type: 'POST',
            contentType: 'application/json',
            success: function(data) {
                if (data.success) {
                    // Redirect to the Index action of the Parties controller
                    window.location.href = '/Register/Register';
                } else {
                    alert(data.message); // Show the server error message
                }
            },
            error: function(jqXHR, textStatus, errorThrown) {
                console.error('Error:', textStatus, errorThrown);
                alert('An error occurred while verifying the OTP.');
            }
        });
    }



    function resetForm() {
        clearInterval(timerInterval);
        const otpContainer = document.getElementById("otpContainer");
        otpContainer.style.display = 'none';
        document.getElementById('sendotp').style.display = 'block';
        document.getElementById('loginForm').reset();
    }


    const wrapper = document.querySelector(".wrapper"),
        signupHeader = document.querySelector(".signup header"),
        loginHeader = document.querySelector(".login header");
    loginHeader.addEventListener("click", () => {
        wrapper.classList.add("active");
    });
    signupHeader.addEventListener("click", () => {
        wrapper.classList.remove("active");
    });
</script>
